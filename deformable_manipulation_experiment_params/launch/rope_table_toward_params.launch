<launch>
    <param name="deformable_type" value="rope"/>
    <param name="task_type" value="rope_table_toward"/>

    <param name="rope_com_x" value="-1.2"/>

    <param name="rope_num_links" value="29"/>
    <param name="rope_radius" value="0.01"/>
    <param name="rope_segment_length" value="0.025"/>
    
    <param name="constraint_translational_dir_deformability" value="20.0"/>
    <param name="constraint_translational_dis_deformability" value="3"/>
    <param name="constraint_rotational_deformability" value="20"/>
    <param name="constraint_obstacle_threshold" value="1"/>
    
    <group ns="deform_simulator_node">
        <param name="num_simsteps_per_gripper_command"  value="20"/>
    </group>
</launch>
