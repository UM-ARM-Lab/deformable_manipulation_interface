<launch>
    <arg name="task_type"/>
    <arg name="dim" default="13"/>
    <group ns="classifier">
        <param name="dim"           value="$(arg dim)"                                                      type="int"/>
        <group ns="transformer">
            <rosparam               file="$(find smmap)/logs/$(arg task_type)/$(arg dim)feature.scaler"     command="load"/>
        </group>
    </group>
    <group ns="svm">
        <param name="model_file"    value="$(find smmap)/logs/$(arg task_type)/$(arg dim)feature.model"     type="string"/>
    </group>
    <group ns="torch">
        <param name="model_file"    value="$(find smmap)/logs/$(arg task_type)/$(arg dim)feature_torch_model_cpu.pt" type="string"/>
        <param name="threshold"     value="0.5"                                                             type="double"/>
    </group>
</launch>